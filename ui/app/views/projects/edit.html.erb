<h1>Edit project</h1>
 
<%= render 'form' %>

<h2>Add a source</h2>
<%= form_for([@project, @project.sources.build]) do |f| %>
  <p>
    <%= f.label :scm_type %><br />
    <%= f.text_field :scm_type %>
  </p>
  <p>
    <%= f.label :url %><br />
    <%= f.text_field :url %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>
 
<%= link_to 'Back', projects_path %>

