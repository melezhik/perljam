
<strong>Build Log </strong> |  <%= link_to 'Back To Project', project_path(@project) %>

<pre>
  <strong>Project ID:</strong> <%= @project.id %>
  <strong>Build ID:</strong> <%= @build.id %>
  <strong>State:</strong> <%= @build.state %>
</pre>

<% if ! @build.comment.nil?  %>
  <strong>Annotation:</strong>
<pre>
<%= @build.comment %>
</pre>
<% end %>


<%- if @build.has_logs? %>
  <strong>Logs.</strong> 
    <pre>
    <% @log_entries.each do |l| %>
        [<%= l.level %>] at <%= l.updated_at %> <%= l.chunk  %>
    <% end %>
    </pre>
<%- end %>

