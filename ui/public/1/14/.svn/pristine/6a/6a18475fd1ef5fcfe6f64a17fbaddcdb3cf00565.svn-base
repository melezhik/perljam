/* sprite PNG32bit Icons */
/* <ins class="sprite32bit size32x32 i32_rm"><i></i></ins> */

.sprite32bit {
  position: relative; overflow: hidden; display: block;
}

.sprite32bit i {
  display: block;
  position: absolute; top: 0px; left: 0px;
  width: 48px;
  height: 5000px;
  background: transparent url(/img/sprite32bit.png) no-repeat 0px 0px;
}

.icon_left, .icon_right {position:relative;}
.icon_left .size48x48, .icon_right .size48x48 {margin-top:-24px;}
.icon_left .size32x32, .icon_right .size32x32 {margin-top:-16px;}
.icon_left .size24x24, .icon_right .size24x24 {margin-top:-12px;}
.icon_left .size16x16, .icon_right .size16x16 {margin-top:-8px;}
.icon_left .sprite32bit, .icon_right .sprite32bit {position:absolute; top:50%;}
.icon_left {padding-left:18px;}
.icon_left .sprite32bit {left:0px;}
.icon_right {padding-right:18px;}
.icon_right .sprite32bit {left:auto; right:0px; }

.inline-block {
  display:-moz-inline-stack; /* Эмуляция для FF2 */
  display: inline-block;
  vertical-align: middle;
  white-space: nowrap;
  }
.inline-block .sprite32bit {
  display:-moz-inline-stack; /* Эмуляция для FF2 */
  display: inline-block;
  vertical-align: middle;
  top:-1px;
  margin:0;
}
.inline-block .sprite32bit i {/* position:relative;  чтобы в FF2 работало смещение фона*/}

.size48x48 {width:48px; height:48px;}
.size32x32 {width:32px; height:32px;}
.size24x24 {width:24px; height:24px;}
.size16x16 {width:16px; height:16px;}

