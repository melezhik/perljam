/* Поле ввода с выпадающим списком */

.dropdown_input {position:relative; display:inline-block;}
.dropdown_input input {
  padding:1px 0px;
  width:100%;
  -moz-box-sizing: border-box;    /* Firefox */
  -webkit-box-sizing: border-box; /* Safari/WebKit/Chrome */
  -ms-box-sizing: border-box;     /* IE8 */
  box-sizing: border-box;         /* W3C Property, Opera */
}
  .dropdown_input input.placeholder {color:gray;}
  .dropdown_input ins.size16x16 {
    cursor:pointer;
    left:auto;
    position:absolute;
    right:3px;
    top:50%;
    margin-top:-8px;
    /*background-color:#fff;*/
    }

  .dropdown_input .inline-block {
    display:inline-block;
    position:absolute;
    left:2px;
    right:auto;
    height:16px;
    top:50%; margin-top:-8px;
    }
    .dropdown_input .inline-block ins.size16x16 {
      display:-moz-inline-stack; /* Эмуляция для FF2 */
      display: inline-block;
      vertical-align: middle;
      top:-1px;
      right:auto;
      position:relative;
      top:auto;
      margin:0;
      cursor:auto;
    }
