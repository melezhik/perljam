table {
  empty-cells:show;
  border-collapse:collapse;
  /* border-collapse: separate; border-spacing: 0px; */
  /* table-layout: fixed; */
  }
colgroup {border-width:0 1px 0 1px; border-color:#B5C6DE; border-style:solid;}

th, td {
  padding:.25em .5em;
  vertical-align:top;
  text-align:auto;
}

  thead td div.colheader {font-weight:bold;}
  thead tr:last-child {border-bottom:1px solid #B5C6DE;}

  tfoot tr:first-child {border-top:1px solid #B5C6DE;}

  tfoot td {font-weight:bold;}

/* Без переноса строк tbody */
table.fixed {
  table-layout:fixed;
  width:100%;
  }
  table.fixed tbody td {
    vertical-align:middle;
  	line-height:1.4em;
    overflow:hidden;
    white-space:nowrap;
    -o-text-overflow:ellipsis;
    text-overflow:ellipsis;
    }
  table.fixed tbody td div {
    overflow:hidden;
    -o-text-overflow:ellipsis;
    text-overflow:ellipsis;
  	}
  table.fixed ins.sprite32bit {/*background-color:red;*/}
  table.fixed tbody td div.wide input {
    vertical-align:baseline;
  	}
  table.fixed tbody .link {white-space:nowrap;}

  col.sorted  {
  	background-color:#f6f6f6;
  }

  col.leads  {
    background-color:#ffd !important;
  }


  table thead div.colheader {display:inline; color:#000;position:static;}

  table thead .sortable {cursor:pointer;}
  table thead .sortable:hover div.colheader {color:#ED5C00 !important}
  table thead .sortable div.colheader {
    /*padding-right:8px;*/
    text-decoration:underline;
  }

