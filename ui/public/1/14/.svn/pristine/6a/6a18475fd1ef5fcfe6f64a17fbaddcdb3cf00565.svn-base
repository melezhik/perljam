/* Calendar layout */

.DateTime {border-collapse:collapse; position:absolute; top:-15px;}
.DateTime td {padding:0;}
.DateTime div.inputData {padding: 0 !important; position:relative; z-index:2; width:100%;}
.DateTime div.inputData input {height:22px; width:11.5em;}
.DateTime div.inputData i {position:absolute; left:auto; right:4px; top:0; width:28px; height:1.7em; background:transparent url(/img/i_calendar.gif) no-repeat -22px 50%;  cursor:pointer; cursor:hand;}
.calendar {border-collapse:collapse; border:2px solid #7094F1;}
.calendar .navYM {padding:2px; color:white; background-color:#7094F1; text-align:center; cursor:default;}
.calendar .navYM div {clear:both; text-align: center;}
.calendar i {position:static; width:11px; height:11px; background:transparent url(/img/i_calendar.gif) no-repeat; cursor:pointer; cursor:hand;}
.calendar i.prev {background-position:0 0; float:left;}
.calendar i.next {background-position:-11px 0; float:right;}
.calendar .month {background-color:white; padding:4px 9px;}
.calendar .month table {border-collapse:separate; empty-cells:show;}
.calendar .month table td {border:1px solid white; text-align:right; cursor:pointer; cursor:hand; padding:1px 2px;}
.calendar .month .empty {cursor:default;}
.calendar .month .week * {color:#999; cursor:default;}
.calendar .month .end {color:#ff6666; }
.calendar .month .current {color:white; background-color:#7094F1; border:1px solid #7094F1;  cursor:default;}
.calendar .month .over {border:1px solid #7094F1; }
.calendar .today td {padding:1px 0 3px; background-color:white; border-top:1px solid gray; text-align:center; cursor:pointer; cursor:hand;}
.calendar .today .over {color:white; background-color:#7094F1; }
#calendarL {  position:fixed;  top:0; left:0;  visibility:hidden;  z-index:99;}

