html {
  overflow-y: scroll; /* чтобы избавиться от горизонтальных скачков при переходе на страницы без скроллбара */
  font-size: 75%;     /* базовый кегль 75% (12px==1em), все остальные наследуют значение = 12px x Xem */
  }
@media all and (min-resolution: 120dpi) {
    html {font-size: 63.2%;} /* пропорционально уменьшаем кегль: 63.2/(120/96) */
    html, x:-moz-any-link {font-size: 79%;} /* выбор несуществующего элемента x с псевдоклассом -moz-any-link. понимают только Gecko-браузеры, остальные проигнорируют всю строчку */
  }

/* ========================================================================== */
/* Reset */
body,address,blockquote,div,p,pre,h1,h2,h3,h4,h5,h6,hr,	  /* блочные элементы */
dd,dl,dt,ul,ol,li,												                /* списки */
a,abbr,acronym,b,big,br,cite,code,del,dfn,em,i,					  /* строчные элементы */
ins,kbd,q,samp,small,span,strong,sub,sup,tt,var,				  /* строчные элементы */
img,object,														                    /* картинки и объекты */
caption,table,tbody,td,tfoot,th,thead,tr,						      /* табличные элементы */
input,textarea,select,button,form,fieldset,legend,label,optgroup 	/* элементы формы */
{
	margin:0px;
	padding:0px;
	vertical-align:baseline;
	text-align:left;
	text-indent:0px;
  font:normal normal normal 1em/normal Arial, Helvetica, sans-serif;
	text-decoration:none;
	list-style-type:none;
}

/* блочные элементы */
h1 {font-size:1.909em; margin:.75em 0 .25em;}
h2 {font-size:1.727em; margin:.75em 0 .25em;}
h3 {font-size:1.636em; margin:.75em 0 .25em;}
h4, h5, h6 {font-size:1.455em; margin:.75em 0 .25em;}

p {margin:.75em 0;}
p:first-child {margin-top:0 !important;}

/* списки */
ol {margin:0px 0px 1.5em 0px; list-style-type: decimal;}
ol li {margin:0px 0px 1em 25px; list-style-type: decimal;}

/* строчные элементы */
strong {font-size:1.364em;}
sup {position:relative; top:-.25em; line-height:normal; font-size:80%;}
a {color:#36c; text-decoration:underline;}
  a:visited {color:#909;}
  a:active {outline:none;} /* убраем рамку только для действий мышкой */
  a:hover,
  a:focus {color:#ed5c00 !important;}

b {font-weight:bold;}

/* картинки и объекты */
img {border:none;}

/* табличные элементы */
table {border-collapse:collapse; empty-cells:show;}
caption,td,th,tr {vertical-align:top;}

/* элементы формы */
textarea {
  vertical-align:top;
  width:100%;
  -moz-box-sizing: border-box;
  -webkit-box-sizing: border-box;
  -ms-box-sizing: border-box;
  box-sizing: border-box;
  }

label,input,select,button {
	vertical-align: middle; /* элементы формы по центру строки */
}
fieldset {
  padding:.75em;
  margin:0px 0em .545em 0em;
  }
optgroup {font-style:normal; font-weight:bold; color:gray; padding-left:3px;}
optgroup > option {color:black;}
/*optgroup > option {-moz-padding-start: 1em;}*/

input[type="radio"],
input[type="checkbox"] {margin-right:.545em;}

input.type-reset,
input.type-submit,
input.type-button,
input[type="reset"],
input[type="submit"],
input[type="button"] {
  padding-left:.545em;
  padding-right:.545em;
  text-align:center;
	overflow:visible;
  }
/* Shared =================================================================== */

.border_box {
  box-sizing: border-box;         /* W3C Property, Opera */
  -moz-box-sizing: border-box;    /* Firefox */
  -webkit-box-sizing: border-box; /* Safari/WebKit/Chrome */
  -ms-box-sizing: border-box;     /* IE8 */
}

.visible {visibility:visible;}
.invisible {visibility:hidden;}
.none   {display:none;}
.block  {display:block;}
.inline-block {
  display:-moz-inline-stack; /* Эмуляция для FF2 */
  display:inline-block;
  }

.clickable {cursor:pointer;}
.link {text-decoration:underline; cursor:pointer;}

.nowrap {white-space:nowrap;}

.float_right {float:right;}
.float_left {float:left;}
.clear	{clear:both;}

.half {width:50%;}
.wide {width:100%;
  box-sizing: border-box;         /* W3C Property, Opera */
  -moz-box-sizing: border-box;    /* Firefox */
  -webkit-box-sizing: border-box; /* Safari/WebKit/Chrome */
  -ms-box-sizing: border-box;     /* IE8 */
}

.text-overflow,
div.overflow {
  display: block;
  overflow:hidden;
  white-space:nowrap;
  position:relative;
  -o-text-overflow:ellipsis;
  text-overflow:ellipsis-word;
  text-overflow:ellipsis;
  }

.r,
.right	{text-align:right;}
.strike {text-decoration:line-through;}

input.placeholder {color:gray;}

/* Pseudo link */
/*
  Псевдоссылка, вызывающая действие без перезагрузки.
  Вместо сплошного подчёркивания используется точечное.

  Для этого отключается text-decoration и с помощью border-bottom и
  двух относительно позиционированных элементов делается его эмуляция.
  Смещение подобрано так, чтобы во всех браузерах положение
  подчёркивания совпадало с text-decoration.

  Код:
  <span class="plink" onclick=""><span>Текст псевдоссылки</span></span>
  <a class="b-pseudo-link" href="" onclick=""><span>Текст псевдоссылки</span></a>
*/

.plink {
  white-space:nowrap;
  text-decoration:none;
  cursor:pointer;
  border-bottom:.091em dotted #36c;
	position:relative;
	top:-.182em;
  }
  .plink span {
  	position:relative;
  	top:2px; /* .182em(2px) при fonsize 1em(11px);*/
  	top:.182em;
  	color:#36c;
  }
  /* Блочная форма ============================================================ */
/* Обрамляющий div для input, textarea нужен чтобы эти поля,
   если они с шириной 100%, в IE не раздвигались вправо при их заполнении.
*/

.mform {padding:1em;}
.mform label,
.mform input,
.mform select,
.mform button {
  /* padding:1px 0px; чтобы были одинаковой высоты  в Chrome */
  vertical-align:middle; /* Чтобы все элементы формы располагались по центру строки */
  overflow:visible; /* Убрали лишние внутренние отступы у кнопок в IE */
}
.mform select {padding:0px; /*border:2px inset threedface;*/}

.mform fieldset {
  display:block;
  border:1px solid silver;
  padding:.545em 2em 1em 0;
  margin:.545em 0 1em;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  border-radius: 4px;
  /*  */
}
.mform fieldset legend {margin:0px 0.818em; padding:.25em; font-size:1.091em; color:#555;}
.mform .legend {margin:.818em 0px .818em 0px; padding:0em; font-size:1.091em; color:#555;}

.mform .separator {
  /*position:relative;*/
  margin:.75em 0;
  padding:0em;
  overflow:hidden;
  height:0px;
  border-style:solid;
