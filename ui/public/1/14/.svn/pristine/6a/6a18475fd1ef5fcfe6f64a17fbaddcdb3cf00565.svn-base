/* Диалоги */
#mask {
  z-index:10;
  position:fixed;
  top:0px;
  left:0px;
  width:100%;
  height:100%;
  background-color:#fff;
  opacity:.6;
}

#dialogs {
  position:fixed;
  top:0; left:0;
  width:100%;
  height:100%;
  overflow:auto;
  z-index:20;
  }

.dialog {
  z-index:30;
  overflow:hidden;
  position:relative;
  top:0px;
  left:50%; width:90%; margin-left:-45%;
  -webkit-box-shadow:0px 1px 12px rgba(0,0,0,.4);
  -moz-box-shadow:0px 1px 12px rgba(0,0,0,.4);
  -moz-border-radius:2px;
  -webkit-border-radius:1px;
  border-radius:1px;
  border:1px solid #3A75BD;
  background-color:#fff;
  }
  .dialog_inner {
  }

  .dialog .panel-header .title {
    display:block;
    position:relative;
    background-color:#5296DE;
    border:0px;
    padding:0px 2.75em 0px 1em;
    margin:0px;
    overflow:hidden;
    height:32px;
  }
    .dialog .panel-header strong {
      display:block;
      overflow:hidden;
      -o-text-overflow:ellipsis;
      text-overflow:ellipsis-word;
      text-overflow:ellipsis;
      width:100%;
      white-space:nowrap;
      font-size:1.25em;
      line-height:2;
      color:white;
      text-shadow: 0px -1px 0px rgba(0,0,0,.2);
    }

