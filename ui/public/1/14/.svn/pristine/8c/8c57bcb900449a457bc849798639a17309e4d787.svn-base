---
  id: create_cpoint
  load_config_file:
    - lines/cpoint/form.yaml
  attributes:
    onsubmit: "return false"

  elements:
    - type: Multi
      elements:
        - type: Submit
          value: Добавить
          attributes_xml:
            onclick: submit_dialog(this.form,'d_cpoint', onShowCPointDialog);

        - type: Reset
          value: Отмена
          attributes_xml:
            onclick: hideDialog('d_cpoint')
